{
    "name": "buildotter/php-maker-standalone",
    "description": "Generate test data builder based on Buildotter Core",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": ["php", "testing", "test", "data", "builder", "maker", "generator"],
    "homepage": "https://github.com/buildotter/php-maker-standalone",
    "authors": [
        {
            "name": "Benjamin Rambaud",
            "email": "beram.job@gmail.com",
            "homepage": "https://brambaud.github.io/"
        }
    ],
    "bin": [
        "bin/buildotter-maker-standalone"
    ],
    "require": {
        "php": "^8.1"
    },
    "suggest": {
        "fakerphp/faker": "Generates fake data for you."
    },
    "config": {
        "platform": {
            "php": "8.1.0"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
